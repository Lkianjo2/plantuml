<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="net.sourceforge.plantuml.eclipse.diagramTextProvider">

      <diagramTextProvider
	    id="net.sourceforge.plantuml.ecore.EcoreClassDiagramTextProvider"
	    providerClass="net.sourceforge.plantuml.ecore.EcoreClassDiagramTextProvider"
      	label="Class diagrams generated from ecore models"
	    >
      </diagramTextProvider>

      <diagramTextProvider
          id="net.sourceforge.plantuml.ecore.EcoreObjectDiagramTextProvider"
          providerClass="net.sourceforge.plantuml.ecore.EcoreObjectDiagramTextProvider"
          priority="default"
	      label="Object diagrams generated from ecore model instances"
      >
      </diagramTextProvider>
   </extension>

</plugin>
